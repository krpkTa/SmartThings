<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SmartThings.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SmartThings"
    
    Shell.FlyoutBehavior="Flyout"
    Title="SmartThings"
    Shell.BackgroundColor="#7593af"
    Shell.TitleColor="#f0ead2"
    Shell.ForegroundColor="#f0ead2">


    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    

</Shell>
